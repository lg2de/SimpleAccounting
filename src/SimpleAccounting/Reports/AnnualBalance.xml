<xEport paperSize="A4" left="20" top="15" bottom="15" right="20">
  <font size="20" />
  <text absX="85" align="center">- @Header_AnnualBalance@ -</text>
  <move relY="10" />
  <font size="10" />
  <text ID="firm" absX="85" align="center">-Firm-</text>
  <move relY="5" />
  <text absX="85" align="center">{yearName}</text>
  <move relY="10" />
  <font size="8" />
  <table lineHeight="4">
    <columns>
      <column width="40">@Word_Incomes@</column>
      <column width="80" />
      <column width="20" align="right" />
      <column width="20" align="right">@Word_Balance@</column>
    </columns>
    <data target="income" />
  </table>
  <move relY="10" />
  <table lineHeight="4">
    <columns>
      <column width="40">@Word_Expenses@</column>
      <column width="80" />
      <column width="20" align="right" />
      <column width="20" align="right">@Word_Balance@</column>
    </columns>
    <data target="expense" />
  </table>
  <move relY="10" />
  <text>@Report_AnnualNetProfitAndLoss@</text>
  <text ID="balance" absX="160" align="right">xxx</text>
  <move relY="20" />
  <table lineHeight="4">
    <columns>
      <column width="40">@Word_Receivables@</column>
      <column width="80" />
      <column width="20" align="right" />
      <column width="20" align="right">@Word_Balance@</column>
    </columns>
    <data target="receivable" />
  </table>
  <move relY="10" />
  <table lineHeight="4">
    <columns>
      <column width="40">@Word_Liabilities@</column>
      <column width="80" />
      <column width="20" align="right" />
      <column width="20" align="right">@Word_Balance@</column>
    </columns>
    <data target="liability" />
  </table>
  <move relY="20" />
  <table lineHeight="4">
    <columns>
      <column width="40">@Word_Asset@</column>
      <column width="80" />
      <column width="20" align="right" />
      <column width="20" align="right">@Word_Balance@</column>
    </columns>
    <data target="asset" />
  </table>
  <move relY="10" />
  <text ID="date" />
</xEport>
