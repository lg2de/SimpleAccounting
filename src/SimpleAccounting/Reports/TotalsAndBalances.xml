<xEport paperSize="A4" landscape="true" left="13" top="15" bottom="15" right="13">
  <pageTexts>
    <font size="8">
      <text ID="pageTitle" absX="135" absY="-5" align="center">- @Header_TotalsAndBalances@ -</text>
      <text absX="135" absY="180" align="center">- @Word_Page@ {page} -</text>
    </font>
  </pageTexts>
  <font size="20" />
  <text ID="title" absX="135" align="center">@Header_TotalsAndBalances@</text>
  <move relY="10" />
  <font size="10" />
  <text ID="firm" absX="135" align="center">-Firm-</text>
  <move relY="5" />
  <text ID="range" absX="135" align="center">-TimeRange-</text>
  <move relY="10" />
  <font size="8" />
  <table lineHeight="4">
    <columns lineHeight="8">
      <column width="20" rightLine="true">@Word_AccountNumber_Wrapped@</column>
      <column width="80" rightLine="true">@Word_AccountName@</column>
      <column width="20" rightLine="true">@Report_LastBooking_Wrapped@</column>
      <column width="25" align="right">
@Report_OpeningAssets@</column>
      <column width="25" rightLine="true" align="right">@Word_OpeningBalance@
@Report_OpeningLiabilities@</column>
      <column width="25" align="right">
@Word_Debit@</column>
      <column width="25" rightLine="true" align="right">@Report_TotalsInTimeRange@
@Word_Credit@</column>
      <column width="25" align="right">
@Word_Debit@</column>
      <column width="25" align="right">@Report_BalanceAtTheEnd@
@Word_Credit@</column>
    </columns>
    <data />
  </table>
  <signatures />
  <move relY="10" />
  <text ID="date" />
</xEport>
